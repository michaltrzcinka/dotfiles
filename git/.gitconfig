[alias]
  st=status
  co=checkout
  cp=cherry-pick
  br=branch
  aa=add -A
  c=commit
  cm=commit -m
  cam=commit -am
  pf=push --force-with-lease

[user]
	name = Michal Trzcinka
	email = michal@trzcinka.pl
[push]
	default = current
[core]
	excludesfile = /Users/michal/.gitignore_global
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
